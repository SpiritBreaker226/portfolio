{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jstats.me/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@vercel/analytics": "^1.0.0",
    "axios": "0.27.2",
    "chart.js": "^4.0.1",
    "concurrently": "^7.6.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-dropdown-select": "^4.9.0",
    "react-router-dom": "^6.11.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26"
  },
  "scripts": {
    "start": "concurrently \"yarn:start:*(!dev)\"",
    "start:frontend": "react-scripts start",
    "start:backend": "cd ./thin-backend && yarn start",
    "start:dev": "concurrently --kill-others \"yarn start:frontend\" \"yarn start:dev:backend\"",
    "start:dev:backend": "cd ./thin-backend && yarn dev",
    "build": "concurrently \"yarn:build:*\"",
    "build:frontend": "cp .env.example .env && yarn test && react-scripts build",
    "build:backend": "cd ./thin-backend && yarn build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
